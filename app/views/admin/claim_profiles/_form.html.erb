<%= form_with(model: [:admin, claim_profile_request], url: admin_claim_profiles_url, class: "flex w-full", method: 'POST' ,data: {type: "html", action: "ajax:error->message#errorHandler ajax:success->message#success" }) do |form| %>
  <%= form.hidden_field :user_id %>
  <%= form.hidden_field :id %>
  <div class="flex-1 mb-6 mr-4">
    <div class="bg-white border pb-16 rounded shadow p-5">
      <div class="w-full">
        <div class="input-group ml-4">
          <label class="label text-gray-600" for="doctor_name">Name<span class="text-red-700 pl-1">*</span></label>
          <%= form.text_field :name, :class => "input bg-white", :autocomplete => "off", disabled: true%>
        </div>
        <div class="input-group ml-4">
          <label class="label text-gray-600" for="doctor_email">Email<span class="text-red-700 pl-1">*</span></label>
          <%= form.text_field :email, :class => "input bg-white", :autocomplete => "off", disabled: true %>
        </div>
      </div>
    </div>
  </div>
<% end %>
